<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
  <mime-type type="audio/x-2sf">
   <glob pattern="*.2sf"/>
   <alias type="audio/2sf"/>
   <alias type="audio/mini2sf"/>
   <generic-icon name="audio-x-generic"/>
   <comment>Nintendo DS Sound Format</comment>
   <comment xml:lang="pt_BR">Arquivo de Som Nintendo DS</comment>
  </mime-type>
  <mime-type type="audio/x-dsf">
   <glob pattern="*.dsf"/>
   <glob pattern="*.minidsf"/>
   <alias type="audio/dsf"/>
   <generic-icon name="audio-x-generic"/>
   <comment>Dreamcast Sound Format</comment>
   <comment xml:lang="pt_BR">Arquivo de Som Dreamcast</comment>
  </mime-type>
  <mime-type type="audio/x-gsf">
   <glob pattern="*.gsf"/>
   <glob pattern="*.minigsf"/>
   <alias type="audio/gsf"/>
   <generic-icon name="audio-x-generic"/>
   <comment>Game Boy Advance Sound Format</comment>
   <comment xml:lang="pt_BR">Arquivo de Som Game Boy Advance</comment>
  </mime-type>
  <mime-type type="audio/x-gym">
   <glob pattern="*.gym"/>
   <alias type="audio/gym"/>
   <generic-icon name="audio-x-generic"/>
   <comment>GYM Sound File</comment>
   <comment xml:lang="pt_BR">Arquivo de Som GYM</comment>
  </mime-type>
  <mime-type type="audio/x-nsf">
   <glob pattern="*.nsf"/>
   <glob pattern="*.nsfe"/>
   <alias type="audio/nsf"/>
   <generic-icon name="audio-x-generic"/>
   <comment>NES Sound Format</comment>
   <comment xml:lang="pt_BR">Arquivo de Som NES</comment>
  </mime-type>
  <mime-type type="audio/x-psf">
   <glob pattern="*.psf"/>
   <glob pattern="*.minipsf"/>
   <glob pattern="*.psf2"/>
   <glob pattern="*.minipsf2"/>
   <alias type="audio/psf"/>
   <generic-icon name="audio-x-generic"/>
   <comment>Playstation Sound Format</comment>
   <comment xml:lang="pt_BR">Arquivo de Som Playstation</comment>
  </mime-type>
  <mime-type type="audio/x-qsf">
   <glob pattern="*.qsf"/>
   <glob pattern="*.miniqsf"/>
   <alias type="audio/qsf"/>
   <generic-icon name="audio-x-generic"/>
   <comment>Capcom Q-Sound Format</comment>
   <comment xml:lang="pt_BR">Arquivo de Som CapcomQ-Sound</comment>
  </mime-type>
  <mime-type type="audio/x-ssf">
   <glob pattern="*.ssf"/>
   <glob pattern="*.minissf"/>
   <alias type="audio/ssf"/>
   <generic-icon name="audio-x-generic"/>
   <comment>Sega Saturn Sound Format</comment>
   <comment xml:lang="pt_BR">Arquivo de Som Sega Saturn</comment>
  </mime-type>
  <mime-type type="audio/x-spc">
    <magic priority="50">
      <match value="SNES-SPC700 Sound File Data" type="string" offset="0"/>
    </magic>
   <glob pattern="*.spc"/>
   <alias type="audio/spc"/>
   <generic-icon name="audio-x-generic"/>
   <comment>SPC700 Sound File</comment>
   <comment xml:lang="pt_BR">Arquivo de Som SPC700</comment>
  </mime-type>
  <mime-type type="audio/x-usf">
   <glob pattern="*.usf"/>
   <glob pattern="*.miniusf"/>
   <alias type="audio/usf"/>
   <generic-icon name="audio-x-generic"/>
   <comment>Nintendo Ultra 64 Sound Format</comment>
   <comment xml:lang="pt_BR">Arquivo de Som Nintendo Ultra 64</comment>
  </mime-type>
</mime-info>
